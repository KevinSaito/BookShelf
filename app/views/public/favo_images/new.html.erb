<h1>新規投稿画面</h1>
<% if @favo_image.errors.any? %>
  <%= @favo_image.errors.count %>件のエラーが発生しました
  <ul>
    <% @post_image.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
  </ul>
<% end %>
<%= form_with model:@favo_image do |f| %>
  <p>画像投稿</p>
  <%= f.file_field :image,accept: "image/*" %>
  <p>場所</p>
  <%= f.text_field :location %>
  <p>説明</p>
  <%= f.text_area :caption %>
  <%= f.submit '投稿' %>
<% end %>